<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.spark.bitrade.mapper.dao.SilkDataDistMapper">

    <select id="findByIdAndKey" parameterType="java.lang.String" resultType="com.spark.bitrade.entity.SilkDataDist">
        select * from silk_data_dist where dict_id = #{dictId} and dict_key = #{dictKey}
    </select>

    <select id="findListByIdAndKey" parameterType="java.lang.String" resultType="com.spark.bitrade.entity.SilkDataDist">
        select * from silk_data_dist where dict_id = #{dictId} and dict_key = #{dictKey}
    </select>

    <select id="findByKey" parameterType="java.lang.String" resultType="com.spark.bitrade.entity.SilkDataDist">
        select * from silk_data_dist where dict_key = #{dictKey}
    </select>

</mapper>
